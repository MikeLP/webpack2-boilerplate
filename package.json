{
  "name": "webpack2-babel",
  "version": "2.0.0",
  "description": "Create app based on Webpack2 with Babel",
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/MikeLP/webpack2-boilerplate.git"
  },
  "author": {
    "name": "Yan Zhabin",
    "email": "iyanello@gmail.com",
    "url": "https://github.com/MikeLP"
  },
  "contributors": [{
    "name": "Yan Zhabin",
    "email": "iyanello@gmail.com",
    "url": "https://github.com/MikeLP"
  }],
  "license": "MIT",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "proxy": "https://127.0.0.1:4000",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "normalize.css": "^5.0.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "ansi-html": "^0.0.6",
    "autoprefixer": "^6.6.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-ramda": "^1.1.6",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "6.19.0",
    "babel-plugin-transform-es2015-parameters": "6.21.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-jsx-self": "6.11.0",
    "babel-plugin-transform-react-jsx-source": "6.9.0",
    "babel-plugin-transform-regenerator": "6.21.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-env": "1.1.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "6.20.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "esformatter": "^0.10.0",
    "esformatter-asi": "^0.5.2",
    "esformatter-auto-wrap": "^1.0.0",
    "esformatter-braces": "^1.2.1",
    "esformatter-collapse-objects": "^0.5.1",
    "esformatter-curly": "0.0.2",
    "esformatter-ignore": "^0.1.3",
    "esformatter-jsx": "^7.4.0",
    "esformatter-parseint": "^1.0.3",
    "esformatter-remove-trailing-commas": "^1.0.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-async": "^0.1.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-node": "^3.0.5",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-entities": "^1.2.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-mixins": "^5.4.1",
    "postcss-smart-import": "^0.6.6",
    "precss": "^1.4.0",
    "pushstate-server": "^2.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "resolve-url": "^0.2.1",
    "sockjs-client": "^1.1.1",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.0",
    "stylelint-scss": "^1.4.1",
    "url-loader": "^0.5.7",
    "webpack": "git+https://github.com/webpack/webpack.git",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "scripts": {
    "_compile:server": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/babel webpack.server.babel.js --out-file webpack.server.js",
    "_compile:config:dev": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/babel webpack.config.babel.js --out-file webpack.config.js",
    "_compile:config:prod": "node_modules/.bin/cross-env NODE_ENV=production node_modules/.bin/babel webpack.config.babel.js --out-file webpack.config.js",
    "_webpack:compile": "npm run build:clear && node_modules/.bin/webpack --config webpack.config.js --output-pathinfo --display-error-details",
    "start": "npm run _compile:config:dev && npm run _compile:server && npm run start:dev",
    "start:dev": "node_modules/.bin/cross-env NODE_ENV=development node ./webpack.server.js",
    "start:prod": "node_modules/.bin/cross-env NODE_ENV=production && node_modules/.bin/pushstate-server build",
    "build:clear": "rm -rf build && mkdir build",
    "build": "npm install && npm run lint && npm run _compile:config:prod && npm run _webpack:compile",
    "build:force": "npm install && npm run _compile:config:prod && npm run _webpack:compile",
    "proxy": "node cors-proxy",
    "lint": "npm run lint:css && npm run lint:js",
    "test": "npm run lint",
    "lint:js": "node_modules/.bin/eslint ./src ",
    "lint:css": "node_modules/.bin/stylelint ./src/assets/css/*.scss && node_modules/.bin/stylelint ./src/app/components/**/*.scss && node_modules/.bin/stylelint ./src/app/containers/**/*.scss"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx",
      "esformatter-ignore",
      "esformatter-remove-trailing-commas",
      "esformatter-collapse-objects",
      "esformatter-auto-wrap",
      "esformatter-parseint",
      "esformatter-braces",
      "esformatter-curly",
      "esformatter-asi"
    ],
    "lineBreak": {
      "before": {
        "ArrayExpressionClosing": 1
      },
      "after": {
        "ArrayExpressionOpening": 1,
        "ArrayExpressionComma": 1
      }
    },
    "quotes": {
      "type": "single"
    },
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": false,
      "maxAttrsOnTag": 2,
      "firstAttributeOnSameLine": false,
      "formatJSXExpressions": true,
      "JSXExpressionsSingleLine": true,
      "alignWithFirstAttribute": false,
      "spaceInJSXExpressionContainers": " ",
      "htmlOptions": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "preserve_newlines": true
      }
    },
    "whiteSpace": {
      "before": {
        "PropertyName": 1
      },
      "after": {}
    },
    "autoWrap": {
      "maxLineLength": 120,
      "eclipseCompatible": true
    },
    "collapseObjects": {
      "ObjectExpression": {
        "maxLineLength": 80,
        "maxKeys": 2,
        "maxDepth": 2,
        "forbidden": [
          "FunctionExpression"
        ]
      },
      "ArrayExpression": {
        "maxLineLength": 80,
        "maxKeys": 3,
        "maxDepth": 2,
        "forbidden": [
          "FunctionExpression"
        ]
      }
    }
  }
}
